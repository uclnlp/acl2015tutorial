    "input" : {
      "sessionId" : null,
      "code" : "### Backpropagation Through Structure\n(Goller and Küchler, 1996)\n\n<br />\n<img src=\"../../assets/figures/07/Backprop.png\" height=\"400\">",
      "extraFields" : { }
    }
  }, {
